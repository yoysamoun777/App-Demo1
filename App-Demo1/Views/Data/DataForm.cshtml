
@{
    ViewData["Title"] = "DataForm";
}

<div class="container">
    <div class="row justify-content-center">
        <div class="col-12">
            <table class="table">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Gender</th>
                        <th>Date of Birth</th>
                        <th>Phone</th>
                        <th>Email</th>
                        <th>Address</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model)

                    {
                        <tr>
                            <td>@item.SID</td>
                            <td>@item.Name</td>
                            <td>@item.Gender</td>
                            <td>@item.DOB</td>
                            <td>@item.Phone</td>
                            <td>@item.Email</td>
                            <td>@item.Address</td>
                        </tr>
                    }


                   
                </tbody>
            </table>
        </div>
    </div>

</div>

